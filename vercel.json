{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [".env", ".env.prod"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
  "env": {
    "NUXT_PUBLIC_ENV": "production",
    "NODE_ENV": "production"
  },
  "outputDirectory": ".output",
  "installCommand": "npm ci --prefer-offline --no-audit"
}
